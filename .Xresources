Xft.dpi: 96
Xft.antialias: true
Xft.rgba: none
Xft.hinting: true
Xft.autohint: true
Xft.hintstyle: hintslight

URxvt*font: -misc-fixed-medium-*-*--18-*-*-*-*-*-iso10646-1
URxvt*boldFont: -misc-fixed-bold-*-*--18-*-*-*-*-*-iso10646-1

!URxvt*font: xft:Inconsolata LGC:medium:pixelsize=18:hinting=true
!URxvt.letterSpace: -1

!URxvt*font: xft:Anonymous Pro:medium:pixelsize=18:hinting=true

URxvt*scrollBar_right: true
! URxvt*visualBell: true
URxvt*urgentOnBell: true
URxvt*scrollstyle: plain
URxvt*internalBorder: 0
URxvt*externalBorder: 0
URxvt*fading: 0

! Solarized color scheme for the X Window System
! http://ethanschoonover.com/solarized
! Dark
URxvt*background: #002b36
URxvt*foreground: #839496
URxvt*fadeColor: #002b36
URxvt*cursorColor: #93a1a1
URxvt*scrollColor: #073642
URxvt*pointerColorBackground: #586e75
URxvt*pointerColorForeground: #93a1a1
URxvt*color0: #073642
URxvt*color1: #dc322f
URxvt*color2: #859900
URxvt*color3: #b58900
URxvt*color4: #268bd2
URxvt*color5: #d33682
URxvt*color6: #2aa198
URxvt*color7: #eee8d5
URxvt*color8: #002b36
URxvt*color9: #cb4b16
URxvt*color10: #586e75
URxvt*color11: #657b83
URxvt*color12: #839496
URxvt*color13: #6c71c4
URxvt*color14: #93a1a1
URxvt*color15: #fdf6e3
!URxvt.intensityStyles: false

rofi.width:                          100
rofi.lines:                          15
rofi.columns:                        1
rofi.font:                           mono 12

rofi.color-enabled:                  true
rofi.color-normal:                   #fdf6e3,#002b36,#eee8d5,#586e75,#eee8d5
rofi.color-urgent:                   #fdf6e3,#dc322f,#eee8d5,#dc322f,#fdf6e3
rofi.color-active:                   #fdf6e3,#268bd2,#eee8d5,#268bd2,#fdf6e3
rofi.color-window:                   #fdf6e3,#002b36

rofi.bw:                             0
rofi.location:                       6
rofi.padding:                        5

rofi.case-sensitive:                 false
rofi.levenshtein-sort:               true
rofi.fuzzy:                          true
rofi.tokenize:                       true

rofi.separator-style:                dash

rofi.kb-primary-paste:               Shift+Insert
rofi.kb-secondary-paste:             Control+y,Insert

xterm*background: black
xterm*foreground: gray

