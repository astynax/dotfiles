alias ls='/bin/ls -NFv --color=auto'
alias ll='/bin/ls -NFvlGh --color=auto --time-style=long-iso --group-directories-first'
alias l='/bin/ls -Naf1v'

alias e='emacs'
alias ec='emacsclient -nq'
alias eq='emacs -Q'
alias enq='emacs -Q -nw'

alias grep='grep --color=auto'
alias cat='bat'

alias yank='xsel -bi'
